.wxSearch{
  position: absolute;
  top: 43px;
  left: 0;
  width: 100%;
  height: 100%;
  border-top: 1px #eee solid;
  background-color: rgba(0, 0, 0, 0.2);
  z-index:1000;
}
.wxSearchInner{
  background-color: #fff;
}
.wxSearch-section{
  width: 100%;
  background-color:white; 
  padding-top: 5px;
  padding-bottom: 5px;
  flex-direction: column;
  align-items: center;
  display: flex;
}
.wxSearch-pancel{
  width: 95%;
  display: flex;
  align-self: center;
  flex-direction: row;
}
.wxSearch-input{
  width: 77.5%;
  background-color: rgb(240, 240, 240);
  padding: 5px 10px;
  border-radius: 5px;
  margin-right: 2.5%;
}
.wxSearch-button{
  width: 20%;
  border: none !important;
  color: cornflowerblue !important;
}
.wxSearchTitle{
  display: block;
  padding: 10px 5px 5px 10px;
  font-size: 12px;
  text-align: center;
}
.wxSearchMindKeyItem{
  padding: 10px 5px;
  border-bottom: 1px solid #eee;
  display: flex;
  font-size: 14px;
  color: cornflowerblue;
}
.wxSearchKeyList{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.wxSearchKeyItem{
  flex: 0 0 20%;
  font-size: 14px;
  text-align: center;
  border: 1px solid #eee;
  margin: 5px;
  padding: 5px 10px;
  border-radius: 5px;
}
.wxSearchHistoryItem{
  padding: 10px 5px;
  border-bottom: 1px solid #eee;
  display: flex;
}
.wxSearchHistoryItemText{
  flex: 8;
  font-size: 14px;
}
.wxSearchHistoryItemDel{
  flex: 1;
  font-size: 12px;
  text-align: center;
  padding: 3px 5px;
  border: 1px solid #eee;
  border-radius: 5px;
}
.wxSearchHistoryEmpty{
  text-align: center;
  height: 50px;
  line-height: 50px;
  font-size: 12px;
}
.wxSearchDeleteAllBtn{
  height: 50px;
  line-height: 50px;
  text-align: center;
  font-size: 12px;
}