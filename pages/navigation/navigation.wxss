/* pages/navigation/navigation.wxss */
.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  color: #000;
  z-index: 9999999;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.navbar{
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  position: relative;
}
/* 标题要居中 */
.nav-title {
  white-space: nowrap;
  font-size: 18px;
  color: #2c2b2b;
}

.back_home{
  position: absolute;
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-left: 10px;
  left: 0;
}

.nav-capsule {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 80px;
  height: 20px;
}

.navbar-v-line {
  width: 1px;
  height: 18px;
  background-color: #575757;
  margin-left: 10px;
  margin-right: 10px;
}

.back-pre{
  width: 20px;
  height:20px;
}

.back-home{
  width: 20px;
  height:20px;
}
