/* pages/mappage/mappage.wxss */
.container{
  width: 100%;
  background: firebrick;
  display: flex;
  flex-direction: column;
}
.mapview{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-grow: 1;
  background-color: forestgreen;
  position: relative;
}
.addmap{
  position: absolute;
  top: 20px;
  right: 10px;
  width: 25px;
  height: 25px;
}
.reducemap{
  position: absolute;
  top: 70px;
  right: 10px;
  width: 25px;
  height: 25px;
}